--explosions

function create_explosion(x,y)
 add(explosions,{
  x = x,
  y = y,
  time = 0
 })
 sfx(1)
end

function update_explosions()
 for boom in all(explosions) do
  boom.time += 1
  if boom.time == 13 then
   del(explosions, boom)
  end
 end
end

function draw_explosions()
 for boom in all(explosions) do
 	circ(boom.x, boom.y,
 	 boom.time/3, 8+boom.time%3)
 end
